{
  "name": "kero-adapter",
  "version": "1.5.4",
  "description": "kero adapter",
  "main": "js/index.js",
  "scripts": {
    "watch": " webpack --progress --colors --watch --mode=dev",
    "build": "webpack --colors --progress --mode=build && node version --mode=build",
    "dev": "webpack --progress --colors  --mode=dev && node version --mode=dev",
    "es": "babel js -d lib",
    "dist": "webpack --progress --colors  --mode=dev && webpack --colors --progress --mode=build && node version --mode=dev &&  node version --mode=build",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "product": "npm run build && npm run dev && gulp dist"
  },
  "dependencies": {
    "kero": "^3.4.3",
    "neoui": "^4.1.2",
    "neoui-grid": "^1.0.0",
    "neoui-polyfill": "^1.2.1",
    "neoui-sparrow": "^1.4.2",
    "neoui-tree": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-preset-es2015": "^6.13.2",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-loader": "^1.5.0",
    "gulp": "^3.9.1",
    "mocha": "^3.0.2",
    "webpack": "^1.13.1",
    "yargs": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iuap-design/kero-adapter.git"
  },
  "author": "yonyou FED",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iuap-design/kero-adapter/issues"
  },
  "homepage": "https://github.com/iuap-design/kero-adapter#readme"
}
