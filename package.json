{
  "name": "kero-adapter",
  "version": "3.1.3",
  "description": "kero adapter",
  "main": "js/index.js",
  "scripts": {
    "watch": " webpack --progress --colors --watch --mode=dev",
    "build": "webpack --colors --progress --mode=build && node version --mode=build",
    "dev": "webpack --progress --colors  --mode=dev && node version --mode=dev",
    "es": "babel js -d lib",
    "dist": "webpack --progress --colors  --mode=dev && webpack --colors --progress --mode=build && node version --mode=dev &&  node version --mode=build",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "product": "npm run build && npm run dev && gulp dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && node changeSort.js && node getAllChange.js"
  },
  "dependencies": {
    "kero": "^2.1.3",
    "tinper-neoui": "^3.1.3",
    "tinper-neoui-grid": "^3.1.3",
    "tinper-neoui-polyfill": "^3.1.3",
    "tinper-neoui-tree": "^3.1.3",
    "tinper-sparrow": "^3.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.2.2",
    "eslint-loader": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "mocha": "^3.0.2",
    "webpack": "^1.13.1",
    "yargs": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iuap-design/kero-adapter.git"
  },
  "author": "yonyou FED",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iuap-design/kero-adapter/issues"
  },
  "homepage": "https://github.com/iuap-design/kero-adapter#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
